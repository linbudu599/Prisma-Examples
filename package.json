{
  "name": "prisma-article-example",
  "description": "Prisma: 下一代ORM, 不仅仅是ORM Demo 仓库",
  "version": "1.0.0",
  "scripts": {
    "reset": "rimraf -rf ./**/prisma/client ./**/*.sqlite",
    "flow": "npm run gen:client && npm run build && npm run invoke",
    "studio": "cd ./src/typegraphql-apollo-server && prisma studio",
    "build": "npm run compile && npm run setup",
    "compile": "rimraf -rf dist && tsc && node copy.js",
    "dev:single": "ts-node-dev --respawn --transpile-only ./src/single-model/index.ts",
    "dev:multi": "ts-node-dev --respawn --transpile-only ./src/multi-models/index.ts",
    "dev:advanced": "ts-node-dev --respawn --transpile-only ./src/multi-models-advanced/index.ts",
    "dev:multi-db": "ts-node-dev --respawn --transpile-only ./src/multi-clients/index.ts",
    "dev:with-typeorm": "ts-node-dev --respawn --transpile-only ./src/with-typeorm/index.ts",
    "dev:with-typegoose": "ts-node-dev --respawn --transpile-only ./src/with-typegoose/index.ts",
    "dev:graphql": "yarn init:graphql && ts-node-dev --respawn --transpile-only ./src/typegraphql-apollo-server/server.ts",
    "gen:client:ci": "npm-run-all --print-label --sequential generate:**",
    "gen:client": "npm-run-all --print-label --parallel generate:**",
    "generate:single": "cd ./src/single-model && prisma db push --force-reset --accept-data-loss",
    "generate:multi": "cd ./src/multi-models && prisma db push --force-reset --accept-data-loss",
    "generate:advanced": "cd ./src/multi-models-advanced && prisma db push --force-reset --accept-data-loss",
    "generate:graphql": "cd ./src/typegraphql-apollo-server && prisma db push --force-reset --accept-data-loss",
    "generate:multi-db": "npm run pre-generate:multi-db-key && npm run pre-generate:multi-db-value",
    "generate:with-typeorm": "cd ./src/with-typeorm && prisma db push --force-reset --accept-data-loss",
    "generate:with-typegoose": "cd ./src/with-typegoose && prisma db push --force-reset --accept-data-loss",
    "pre-generate:multi-db-key": "cd ./src/multi-clients && prisma db push --schema=./prisma-key/schema.prisma --preview-feature --force-reset --accept-data-loss --schema=./prisma-key/schema.prisma",
    "pre-generate:multi-db-value": "cd ./src/multi-clients && prisma db push --schema=./prisma-value/schema.prisma --preview-feature --force-reset --accept-data-loss --schema=./prisma-value/schema.prisma",
    "setup": "npm-run-all --print-label --parallel setup:**",
    "setup:single": "cd dist/src/single-model && prisma db push",
    "setup:multi": "cd dist/src/multi-models && prisma db push",
    "setup:advanced": "cd dist/src/multi-models-advanced && prisma db push",
    "setup:graphql": "cd dist/src/typegraphql-apollo-server && prisma db push",
    "setup:multi-db": "npm run pre-setup:multi-db-key && npm run pre-setup:multi-db-value",
    "setup:with-typeorm": "cd dist/src/with-typeorm && prisma db push",
    "setup:with-typegoose": "cd dist/src/with-typegoose && prisma db push",
    "pre-setup:multi-db-key": "cd dist/src/multi-clients && prisma db push --schema=./prisma-key/schema.prisma",
    "pre-setup:multi-db-value": "cd dist/src/multi-clients && prisma db push --schema=./prisma-value/schema.prisma",
    "invoke": "npm-run-all --print-label --sequential invoke:**",
    "invoke:single": "node dist/src/single-model/index.js",
    "invoke:multi": "node dist/src/multi-models/index.js",
    "invoke:advanced": "node dist/src/multi-models-advanced/index.js",
    "invoke:multi-db": "node dist/src/multi-clients/index.js",
    "invoke:with-typeorm": "cross-env NODE_ENV=prod node dist/src/with-typeorm/index.js",
    "invoke:with-typegoose": "node dist/src/with-typegoose/index.js",
    "seed": "cd ./src/typegraphql-apollo-server && prisma db seed --preview-feature",
    "init:graphql": "ts-node ./src/typegraphql-apollo-server/prisma/seed.ts",
    "gen:code": "graphql-codegen --config codegen.yml",
    "gen:genql": "genql --endpoint http://localhost:5999/graphql --output ./src/typegraphql-apollo-server/generated/genql",
    "test:graphql": "yarn init:graphql && jest --forceExit",
    "test:prisma": "jest --forceExit --config jest.config.prisma.ts",
    "pretest": "cd ./src/typegraphql-apollo-server && prisma db push --force-reset --accept-data-loss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linbudu599/Prisma-Article-Example.git"
  },
  "author": "linbudu599",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linbudu599/Prisma-Article-Example/issues"
  },
  "homepage": "https://github.com/linbudu599/Prisma-Article-Example#readme",
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@genql/runtime": "^2.5.0",
    "@prisma/client": "^2.19.0",
    "@typegoose/typegoose": "^7.6.0",
    "apollo-server": "^2.21.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "mongoose": "^5.10.18",
    "pg": "^8.5.1",
    "prisma": "^2.19.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "ts-node-dev": "^1.1.6",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.31",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@genql/cli": "^2.5.0",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/time": "^2.0.2",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-react-apollo": "^2.2.3",
    "@graphql-codegen/typescript-resolvers": "^1.19.0",
    "@types/jest": "^26.0.21",
    "@types/mongoose": "^5.10.4",
    "@types/node": "^16.7.10",
    "@types/uuid": "^8.3.0",
    "apollo-server-testing": "^2.21.2",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "esbuild-node-tsc": "^1.2.0",
    "fs-extra": "^9.1.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ora": "^5.4.0",
    "react": "^17.0.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "typegraphql-prisma": "^0.12.0"
  }
}
